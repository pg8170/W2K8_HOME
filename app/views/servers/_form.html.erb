	<div class = 'row' >
	 <div class = 'col-xs-12' >
		<%= form_for(@server, :html => {class: "form-horizontal", role: "form"}) do |f|%>
		<div class ="form-group">
		<div class = "control-lable col-sm-2">
			<%=f.label :HostName%>
		</div>
		<div class="col-sm-8">
			<%=f.text_field :hostname, class: "form-control", placeholder: "Enter HostName",autofocus: true%>
		</div>
		</div>
		<div class ="form-group">
		<div class = "control-lable col-sm-2">
		<%=f.label :Admin%>
		</div>
		<div class="col-sm-8">
		<%=f.text_field :admin ,rows: 10,class: "form-control",placeholder: "Enter Admin of the server"%>	
		</div>
		</div>
   
    <div class ="form-group">
		<div class = "control-lable col-sm-2">
		<%=f.label :MigrationDate%>
		</div>
		<div class="col-sm-8">
		<%=f.text_field :migrationdate ,rows: 10,class: "form-control",placeholder: "Enter Migrationdate of the server"%>	
		</div>
		</div>
  
     <div class ="form-group">
		<div class = "control-lable col-sm-2">
		<%=f.label :DelayReason%>
		</div>
   	<div class="col-sm-8">
		<%=f.text_field :delayreason ,rows: 10,class: "form-control",placeholder: "Enter Delay Reason"%>	
		</div>
		</div>
   
   <div class ="form-group">
		<div class = "control-lable col-sm-2">
		<%=f.label :Status%>
		</div>
		<div class="col-sm-8">
		<%=f.text_field :status ,rows: 10,class: "form-control",placeholder: "Enter the status of server"%>	
		</div>
		</div>

   
		<div class ="form-group">
		<div class = "col-sm-offset-2 col-sm-10">
		<%=f.submit class: 'btn btn-primary btn-lg'%>
		</div>
		</div>
   
	
		<%end%>
		</div>
	</div>